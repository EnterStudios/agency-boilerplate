---
url: "#image-link"
picture: {}
headline: {}
---
{{#mixin "elements/link" this}}
  {{#content "attributes"}}
    data-partial="elements/link/image"
  {{/content}}
  {{#content "title"}}
    {{{mixin "elements/picture" picture}}}
    {{#block "placeholder"}}
      {{{mixin "elements/headline/h4" headline}}}
    {{/block}}
  {{/content}}
{{/mixin}}
